{
  "name": "reebok-ic-app",
  "version": "1.0.0",
  "description": "Aplicación especializada en gestión de Códigos IC para Reebok",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rimraf .next && next dev",
    "dev:server": "node server.js",
    "build": "prisma generate && next build",
    "build:with-polyfill": "node -p \"global.Request = class MockRequest { constructor(url, opts = {}) { this.url = url; this.method = opts.method || 'GET'; } async json() { return {}; } }; 'Polyfill loaded'\" && prisma generate && next build --no-lint",
    "start": "next start",
    "start:server": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next node_modules/.cache",
    "clean:all": "rimraf .next node_modules/.cache node_modules/.prisma",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.2",
    "@mui/icons-material": "^5.15.1",
    "@mui/material": "^5.15.1",
    "@mui/x-data-grid": "^6.18.3",
    "@mui/x-data-grid-pro": "^6.18.3",
    "@mui/x-date-pickers": "^6.18.3",
    "@prisma/client": "^6.18.0",
    "@types/bcryptjs": "^2.4.4",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.6",
    "dotenv": "^17.2.3",
    "lodash": "^4.17.21",
    "mssql": "^12.0.0",
    "next": "14.0.4",
    "odbc": "^2.4.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "recharts": "^2.8.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "ignore-loader": "^0.1.2",
    "node-fetch": "^2.7.0",
    "prisma": "^6.18.0",
    "rimraf": "^6.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "reebok",
    "codigos-ic",
    "inventory",
    "nextjs",
    "material-ui",
    "sap-hana"
  ]
}
